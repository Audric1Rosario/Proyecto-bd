<Window x:Class="Proyecto.Vistas.winHistorial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto.Vistas"
        mc:Ignorable="d"
        Title="Historial de procedimientos." Height="600" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="/Proyecto;component/Images/hospital (1).png">
    <Grid>
        <Grid Height="50" VerticalAlignment="Top">
            <Label Content="Listado de procedimientos." HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Times New Roman" FontSize="24" FontWeight="Bold" FontStyle="Italic" Margin="0"/>
        </Grid>
        <DataGrid x:Name="dataGrid" Margin="0,50,0,75"/>
        <Grid Height="75" VerticalAlignment="Bottom">
            <Button x:Name="btnSalir" Content="Salir" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="150" Margin="0,0,10,10" Height="50" Click="btnSalir_Click"/>
        </Grid>

    </Grid>
</Window>
